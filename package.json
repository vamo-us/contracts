{
  "name": "@vamous/interfaces",
  "version": "1.0.0",
  "private": false,
  "author": "Vamous",
  "license": "MIT",
  "typings": "dist/index.d.ts",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vamo-us/interfaces.git"
  },
  "release": {
